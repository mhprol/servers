{
  "name": "@modelcontextprotocol/server-memory-dynamic",
  "version": "2.0.0",
  "description": "MCP server for enabling memory for Claude through a knowledge graph with index-first architecture and dynamic memory file support",
  "license": "MIT",
  "author": "Anthropic, PBC (https://anthropic.com)",
  "contributors": [
    "Matheus Prol (https://github.com/mhprol)"
  ],
  "homepage": "https://modelcontextprotocol.io",
